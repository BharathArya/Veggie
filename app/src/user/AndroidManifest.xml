<manifest xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        xmlns:tools="http://schemas.android.com/tools">
        <activity
            android:name="com.greentopli.app.user.PurchaseManagerActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            tools:replace="name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.greentopli.app.AuthenticatorActivity">
        </activity>

        <provider
            android:name="com.greentopli.core.storage.DatabaseProvider"
            android:authorities="com.greentopli.core.storage"
            android:exported="false" />
    </application>
</manifest>
